{
  "name": "@launchql/ast",
  "version": "13.0.4",
  "description": "PostgreSQL AST utils",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "postgres",
    "AST",
